<main>
<ul  class = "user-list">
     <% @users.each do |user| %>
        <li class = "user-container">
            <div class = "user-picture-container">
              <img src="<%= user.photo %>" alt="user.name">
            </div>
            <div class = "user-info">
                <p class = user-name><%= user.name %></p>
                <p class = user-detail> Number of post: <%= user.bio %></p>
            </div>
        </li>
    <% end %>
</ul>
</main>
